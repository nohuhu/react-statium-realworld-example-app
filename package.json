{
  "name": "react-statium-realworld-example",
  "version": "0.1.0",
  "dependencies": {
    "markdown-it": "^12.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "statium": "^0.5.2",
    "urlito": "^0.2.0"
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "identity-obj-proxy": "^3.0.0"
  },
  "scripts": {
    "start": "PORT=4884 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^test/(.+)$": "<rootDir>/test/$1",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
